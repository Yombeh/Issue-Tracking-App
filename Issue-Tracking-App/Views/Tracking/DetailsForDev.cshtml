@model Issue_Tracking_App.Models.DetailsForDevViewModel
<h2 style="margin-top: 80px;"></h2>
<table class="table table-bordered table-striped">
    <tr>
        <th>User</th>
        <td>@Model.UserReports.userName</td>
    </tr>

    <tr>
        <th>Application</th>
        <td>@Model.Application</td>
    </tr>

    <tr>
        <th>Description</th>
        <td>@Model.UserReports.Description</td>
    </tr>

    <tr>
        <th>Date</th>
        <td>@Model.UserReports.DatePicker</td>
    </tr>

    <tr>
        <th>Created</th>
        <td>@Model.UserReports.CreatedDate</td>
    </tr>

    <tr>
        <th>Severity</th>
        <td>@Model.severity</td>
    </tr>

    <tr>
        <th>Admin</th>
        <td>@Model.reportDetails.Comment</td>
    </tr>
</table>
<form asp-action="DetailsForDev" asp-controller="Tracking">

<div class="mt-3">
    <label class="form-label">How was it Solved</label>
    <input type="text" class="form-control" asp-for="devComment">
</div>

<div class="mt-3">
    <button class=" btn btn-success" type="submit">Done</button>
</div>
</form>
